    "def u ( n ) :\n",
    "    # input : an integer n\n",
    "    # stop condition -> extremely important\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    # recursive call\n",
    "    else :\n",
    "        return n * u (n -1)\n",
    "print ( u (5) ) # should print 120... but why ?"
   ]
